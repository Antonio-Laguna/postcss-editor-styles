{
  "name": "postcss-scope-to",
  "version": "0.1.0",
  "description": "Remove your CSS from the global scope ",
  "author": "m-e-h <info@martyhelmick.com>",
  "license": "CC0-1.0",
  "repository": "m-e-h/postcss-scope-to",
  "homepage": "https://github.com/m-e-h/postcss-scope-to#readme",
  "bugs": "https://github.com/m-e-h/postcss-scope-to/issues",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm test",
    "test": "echo 'Running tests...'; npm run test:js && npm run test:tape",
    "test:js": "eslint *.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "postcss-tape"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "postcss": "^7.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-dev": "^2.0.0",
    "postcss-tape": "^2.2.0",
    "pre-commit": "^1.2.2"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "wordpress"
  ]
}
